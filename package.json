{
  "name": "ts-runtime",
  "version": "0.0.1",
  "description": "Runtime type checks for TypeScript",
  "main": "dist/index",
  "bin": {
    "tsr": "dist/bin/index"
  },
  "repository": "https://github.com/fabiandev/ts-runtime.git",
  "author": "Fabian Pirklbauer <hi@fabiandev.io>",
  "license": "MIT",
  "scripts": {
    "build": "tsc --rootDir src --outDir dist --module CommonJS --target es6 --skipLibCheck"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "flow-runtime": "^0.2.1",
    "log-symbols": "^1.0.2",
    "node-status": "^1.0.0",
    "ora": "^1.1.0",
    "rimraf": "^2.6.1",
    "ts-type-info": "^6.2.2",
    "typescript": "next"
  },
  "devDependencies": {
    "@types/chalk": "^0.4.31",
    "@types/commander": "^2.3.31",
    "@types/escodegen": "^0.0.6",
    "@types/esprima": "^2.1.33",
    "@types/node": "^7.0.2",
    "@types/ora": "^0.3.31",
    "@types/rimraf": "^0.0.28"
  }
}
