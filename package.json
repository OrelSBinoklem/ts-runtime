{
  "name": "ts-runtime",
  "version": "0.1.28",
  "description": "Runtime type checks for TypeScript",
  "main": "index.js",
  "bin": {
    "tsr": "bin/index.js"
  },
  "repository": "https://github.com/fabiandev/ts-runtime.git",
  "author": "Fabian Pirklbauer <hi@fabiandev.io>",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "test": "NODE_ENV=test TS_NODE_CACHE=false ./node_modules/mocha/bin/_mocha",
    "cover": "NODE_ENV=test TS_NODE_CACHE=false ./node_modules/.bin/nyc npm test",
    "coveralls": "./node_modules/.bin/nyc report --reporter=text-lcov | ./node_modules/.bin/coveralls",
    "build": "./node_modules/.bin/tsc --rootDir src --outDir dist --module CommonJS --target es5 --skipLibCheck && cp package.json dist/package.json && cp README.md dist/README.md && cp LICENSE dist/LICENSE",
    "build:docs": "node_modules/.bin/gulp --gulpfile src/docs/build/gulpfile.js build",
    "build:tsr": "ts-node ./src/bin/index src/index src/lib/index src/bin/index src/bin/process --moduleAlias --compilerOptions '{\"outDir\": \"dist-tsr\", \"strictNullChecks\": false, \"moduleResolution\": \"Node\", \"module\": \"commonjs\", \"target\": \"ES2015\", \"lib\": [\"ESNext\"]}'"
  },
  "_moduleAliases": {
    "ts-runtime": "dist"
  },
  "dependencies": {
    "chalk": "^2.0.1",
    "commander": "^2.9.0",
    "commondir": "^1.0.1",
    "flow-runtime": "^0.13.0",
    "ora": "^1.2.0",
    "path-browserify": "^0.0.0",
    "pretty-time": "^0.2.0",
    "regenerator-runtime": "^0.10.5",
    "rimraf": "^2.6.1",
    "typescript": "^2.4.1"
  },
  "devDependencies": {
    "@types/chalk": "^0.4.31",
    "@types/commander": "^2.3.31",
    "@types/expect.js": "^0.3.29",
    "@types/lodash.debounce": "^4.0.2",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.16",
    "@types/rimraf": "^0.0.28",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "expect.js": "^0.3.1",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-clean-css": "^3.7.0",
    "gulp-less": "^3.3.2",
    "gulp-preprocess": "^2.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.6.0",
    "lodash.debounce": "^4.0.8",
    "mocha": "^3.4.2",
    "module-alias": "^2.0.0",
    "monaco-editor": "^0.10.0",
    "monaco-typescript": "git+https://git@github.com/Microsoft/monaco-typescript",
    "normalize.css": "^7.0.0",
    "null-loader": "^0.1.1",
    "nyc": "^11.0.3",
    "raw-loader": "^0.5.1",
    "source-map-loader": "^0.2.1",
    "source-map-support": "^0.4.15",
    "ts-loader": "^2.2.2",
    "ts-node": "^3.3.0",
    "webpack": "^2.2.0",
    "worker-loader": "^0.8.1"
  }
}
