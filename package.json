{
  "name": "ts-runtime",
  "version": "0.0.1",
  "description": "Runtime type checks for TypeScript",
  "main": "dist/index",
  "bin": {
    "tsr": "dist/bin/index"
  },
  "repository": "https://github.com/fabiandev/ts-runtime.git",
  "author": "Fabian Pirklbauer <hi@fabiandev.io>",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "build": "./node_modules/.bin/tsc --rootDir src --outDir dist --module CommonJS --target es6 --skipLibCheck",
    "build:tsr": "ts-node ./src/bin/index src/index.ts -s 25 -fkc '{\"outDir\": \"dist-tsr\", \"strictNullChecks\": false, \"moduleResolution\": \"Node\", \"module\": \"commonjs\", \"target\": \"ES5\", \"lib\": [\"ESNext\"]}'"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "log-symbols": "^1.0.2",
    "node-status": "^1.0.0",
    "ora": "^1.2.0",
    "pretty-time": "^0.2.0",
    "rimraf": "^2.6.1",
    "source-map-support": "0.4.3",
    "typescript": "next"
  },
  "devDependencies": {
    "@types/chalk": "^0.4.31",
    "@types/commander": "^2.3.31",
    "@types/escodegen": "^0.0.6",
    "@types/esprima": "^2.1.33",
    "@types/node": "^7.0.2",
    "@types/ora": "^0.3.31",
    "@types/rimraf": "^0.0.28",
    "flow-runtime": "^0.12.0"
  }
}
